#include<iostream>
#include<cmath> 
using namespace std;
int main() {
    double num;
    cout << "Enter a number: ";
    cin >> num;
    if (num >= 0) {
        double result = sqrt(num); 
        cout << "Square root of " << num << " is " << result << endl;
    } else {
        cout << "it is not defined in real numbers." << endl;
    }
    return 0;
}
