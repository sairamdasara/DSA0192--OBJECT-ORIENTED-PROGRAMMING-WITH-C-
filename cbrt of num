#include<iostream>
#include<cmath> 
using namespace std;

int main() {
    double num;
    cout << "Enter a number: ";
    cin >> num;
    double result = cbrt(num);
    if (num == 0) {
        cout << "The cube root " << endl;
    } else {
        cout << "Cube root of " << num << " is " << result << endl;
    }

    return 0;
}
